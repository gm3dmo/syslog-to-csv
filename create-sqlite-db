rm logs.db
sqlite3 logs.db << EOF
.separator ","
.import github-logs/exceptions.log.csv exceptions
.import github-logs/unicorn.log.csv unicorn
.import github-logs/gitauth.log.csv gitauth
.import github-logs/auth.log.csv auth
.import system-logs/split-logs-syslog/babeld.log.csv babeld

.import github-logs/exceptions.log.1.csv exceptions1
.import github-logs/unicorn.log.1.csv unicorn1
.import github-logs/gitauth.log.1.csv gitauth1
.import github-logs/auth.log.1.csv auth1
.import system-logs/split-logs-syslog/babeld.log.1.csv babeld1
EOF
